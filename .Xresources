URxvt.perl-ext-common:          default,matcher,resize-font
URxvt.urlLauncher:              /usr/bin/xdg-open
URxvt.matcher.button:           1
URxvt.url-select.underline:     true

!!URxvt Settings
URxvt.colorUL:                  #777777
URxvt.imLocale:                 en_AU.UTF-8
URxvt.scrollBar:                false
URxvt.cursorBlink:              true
URxvt.cursorUnderline:          false
URxvt.saveLines:                10024
URxvt.shading:                  60
URxvt.depth:                    32
URxvt.background:               rgba:0000/0000/0200/c800

!!URxvt Fonts
URxvt.font:                     xft:Misc Tamsyn:style=Regular:size=9
URxvt.boldFont:                 xft:Misc Tamsyn:style=Bold:size=9

!!URxvt Font Settings
URxvt.autohint:                 0
URxvt.dpi:                      96
URxvt.lcdfilter:                lcddefault
URxvt.hintstyle:                hintfull
URxvt.hinting:                  1
URxvt.antialias:                1
URxvt.rgba:                     rgb

!!General Xft font settings
Xft.hinting:                    true
Xft.hintstyle:                  hintfull
Xft.antialias:                  rgba
Xft.rgba:                       rgb

!!Font resize settings
URxvt.resize-font.smaller:      C-Down
URxvt.resize-font.bigger:       C-Up

URxvt.clipboard.autocopy:       true
